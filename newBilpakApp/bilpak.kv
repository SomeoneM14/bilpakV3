                            theme_bg_color: "Custom"
                            md_bg_color: self.theme_cls.surfaceContainerLowColor
                            ripple_effect: False

                            MDListItemSupportingText:
                                text: "Stuff i stole from Alon"

                            TrailingPressedIconButton:
                                id: chevron
                                icon: "chevron-right"
                                on_release: app.tap_expansion_chevron(panel, chevron)

